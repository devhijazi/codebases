{
  "name": "payment-service",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marinetesio/payment-service.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "eslint ./ --ext \"js,ts\"",
    "lint:fix": "eslint ./ --ext \"js,ts\" --fix",
    "format": "prettier --check \"./**/*.{js,ts}\"",
    "format:fix": "prettier --write \"./**/*.{js,ts}\"",
    "fix": "run-s format:fix lint:fix",
    "clean": "rimraf coverage dist",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "build:files": "node esbuild.config.js",
    "build": "run-s clean build:files",
    "start": "cross-env NODE_ENV=production node dist/index"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.2",
    "@marinetesio/asaas-sdk": "^0.1.0",
    "@marinetesio/database": "^0.1.0",
    "@marinetesio/errors": "^0.1.0",
    "@marinetesio/logger": "^0.0.1",
    "@marinetesio/protos": "^0.1.0",
    "@marinetesio/types": "^0.1.0",
    "@protobuf-ts/grpc-backend": "^2.9.1",
    "@protobuf-ts/runtime-rpc": "^2.9.1",
    "@supercharge/strings": "^2.0.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.2.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.3.4",
    "socket.io": "^4.7.2",
    "typeorm": "^0.2.41",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@marinetesio/eslint-config": "^0.0.1",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "8.5.1",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "esbuild": "^0.15.13",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "3.4.1",
    "fast-glob": "^3.2.12",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.7.1",
    "tsx": "^3.12.7",
    "typescript": "4.8.3"
  }
}
